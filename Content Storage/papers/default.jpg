<svg xmlns="http://www.w3.org/2000/svg" width="800" height="450" viewBox="0 0 800 450">
  <!-- Background -->
  <rect width="800" height="450" fill="#0a0a0a"/>
  
  <!-- Grid Pattern -->
  <defs>
    <pattern id="grid-research" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#9933CC" stroke-width="0.5" opacity="0.1"/>
    </pattern>
  </defs>
  <rect width="800" height="450" fill="url(#grid-research)"/>
  
  <!-- Research Icon -->
  <g transform="translate(400, 225)">
    <circle cx="0" cy="0" r="60" fill="none" stroke="#9933CC" stroke-width="3" opacity="0.3"/>
    
    <!-- Flask -->
    <path d="M -20 -40 L -20 -10 L -35 30 L -35 40 L 35 40 L 35 30 L 20 -10 L 20 -40 Z" 
          fill="none" stroke="#9933CC" stroke-width="3" opacity="0.8"/>
    <path d="M -20 -10 L -30 20 L -30 35 L 30 35 L 30 20 L 20 -10" fill="#9933CC" opacity="0.3"/>
    <rect x="-25" y="-45" width="50" height="5" fill="#9933CC"/>
    
    <!-- Bubbles -->
    <circle cx="-10" cy="15" r="4" fill="#9933CC" opacity="0.6"/>
    <circle cx="5" cy="20" r="3" fill="#9933CC" opacity="0.6"/>
    <circle cx="12" cy="10" r="5" fill="#9933CC" opacity="0.6"/>
  </g>
  
  <!-- Text -->
  <text x="400" y="350" font-family="'Inter', sans-serif" font-size="32" font-weight="700" fill="#9933CC" text-anchor="middle">
    RESEARCH PAPER
  </text>
  <text x="400" y="385" font-family="'Inter', sans-serif" font-size="18" fill="#666" text-anchor="middle">
    Default Cover
  </text>
</svg>
